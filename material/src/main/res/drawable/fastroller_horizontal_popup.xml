<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">

            <solid android:color="#1A73E8" />
            <size
                android:width="64dp"
                android:height="64dp" />
        </shape>
    </item>

    <item
        android:left="20dp"
        android:right="10dp"
        android:top="10dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <size
                    android:width="10dp"
                    android:height="10dp" />
                <solid android:color="#1A73E8" />
            </shape>
        </rotate>
    </item>


</layer-list>