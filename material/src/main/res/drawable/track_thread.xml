<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">

            <solid android:color="@android:color/transparent" />
            <size
                android:width="12dp"
                android:height="64dp" />
        </shape>
    </item>
    <item
        android:left="6dp"
        android:right="6dp">
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
            <size
                android:width="1dp"
                android:height="64dp" />
        </shape>
    </item>
</layer-list>